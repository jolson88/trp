{
  "name": "jolson88-site-generator",
  "version": "1.0.0",
  "description": "Site generator for The Reasonable Programmer website",
  "main": "index.js",
  "private": "true",
  "scripts": {
    "build": "ts-node src/index.ts _in _out",
    "start": "npm run build && live-server _out",
    "sync": "npm run build && cp -R _out/ ../../jolson88.github.io",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "TypeScript",
    "Node"
  ],
  "author": "Jason Olson",
  "license": "UNLICENSED",
  "devDependencies": {
    "@vitest/coverage-v8": "^0.34.6",
    "live-server": "^1.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}
